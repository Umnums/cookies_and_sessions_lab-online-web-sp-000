<h1>Products In Your Cart!</h1>
<ul>
    <% cart.each do |item|%>
        <li><%= item%></li>
    <% end%>
</ul>

<h1> Add Items to Your Cart! </h1>
<p>
<%= form_tag '/add', method: :get do %>
    <%= label_tag :product, "Add this item to your cart:" %>
    <%= text_field_tag :product%>
    <%= submit_tag "add to cart"%>
<%end%>
</p>

